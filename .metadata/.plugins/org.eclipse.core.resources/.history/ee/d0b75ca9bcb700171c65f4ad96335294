package answermodel;

public class UsingJoinSleep extends Thread {

	@Override
	public void run() {
		for(int i=0;i<10;i++) {
			System.out.println(i*2);
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Thread t = new UsingJoinSleep();
		Thread t1 = new Thread();
		t.start();
	}

}
